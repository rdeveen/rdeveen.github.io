  "version": "0.2.0",
  "configurations": [
    {
      "type": "rdbg",
      "name": "Debug current file with rdbg",
      "request": "launch",
      "script": "${file}",
      "args": [],
      "askParameters": true
    },
    {
      "type": "rdbg",
      "name": "Attach with rdbg",
      "request": "attach"
    },
    {
    "args": [
      "serve",
      "--livereload_port", "35732",
      "--force_polling",
      "--host", "0.0.0.0",
      "--port", "4444",
      "--future",
      "--incremental",
      "--livereload",
      "--drafts",
      "--unpublished"
    ],
    "cwd": "${workspaceRoot}",
    "debugPort": "0",
    "name": "Debug Demo",
    "request": "launch",
    "script": "jekyll",
    "type": "rdbg",
    "useBundler": true,
  }
  ]
}